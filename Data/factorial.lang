func factorial_recursive(n: int) -> int {
	if (n == 0) return 1;
	
	return factorial_recursive(n - 1) * n;
}

func factorial_loop(n: int) -> int {
	let result: int;
	result = 1;

	while (n > 0) {
		result = result * n;

		n = n - 1;
	}
	
	return result;
}

func main() -> int {
	let arg: int;
	arg = 5;
	
	let a: int;
	let b: int;
	
	a = factorial_recursive(arg);
	b = factorial_loop     (arg);

	return a == b;
}
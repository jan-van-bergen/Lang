extern GetStdHandle;
extern WriteFile;

func test(a: int, b: int, c: int, d: int) -> int {
	return a + b + c + d;
}

func main() -> int {
	let std_handle: int;
	std_handle = GetStdHandle(-11);
	
	let str_len: int;
	str_len = test(10, 1, 1, 1);
	
	let bytes_written: int;
	WriteFile(std_handle, "Hallo wereld!", str_len, &bytes_written, 0);
	
	return bytes_written;
}